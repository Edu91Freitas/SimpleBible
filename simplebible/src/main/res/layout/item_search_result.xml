<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  app:cardPreventCornerOverlap="true"
  app:cardUseCompatPadding="true">

  <TextView
    android:id="@+id/itemSearchResultContent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/gapNormal"
    android:layout_marginTop="@dimen/gapSmall"
    android:layout_marginEnd="@dimen/gapNormal"
    android:layout_marginBottom="@dimen/gapSmall"
    android:paddingStart="@dimen/gapSmall"
    android:paddingEnd="@dimen/gapSmall"
    tools:text="@string/defaultVerseReference" />

</androidx.cardview.widget.CardView>
