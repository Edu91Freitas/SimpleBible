<resources>

    <style nameV2="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item nameV2="colorPrimary">@color/primary</item>
        <item nameV2="colorPrimaryDark">@color/primary_dark</item>
        <item nameV2="colorAccent">@color/accent</item>
    </style>

    <style nameV2="AppTheme.NoActionBar">
        <item nameV2="windowActionBar">false</item>
        <item nameV2="windowNoTitle">true</item>
    </style>

    <style nameV2="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style nameV2="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

</resources>
